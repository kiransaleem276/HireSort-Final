@{
    ViewData["Title"] = "View All Resumes";
}

<!-- View All Resume - Vacancy Wise  -->

<div class="container-xxl py-5 mt-5 wow fadeInUp" data-wow-delay="0.1s">
    <div class="container">
        <div class="row gy-5 gx-4">
            <div class="col-lg-12">
                <div class="">
                    <div class="row g-3 my-4">
                        <div class="col-10"><h4 class="mb-4">View All Resumes - Vacancy Wise</h4></div>
                        <div class="col-3 text-end d-none">  <button class="btn btn-primary w-50">Upload Resume</button></div>
                        <div class="col-2 text-end">  <a asp-area="" asp-controller="" asp-action="" class="btn btn-primary w-100">Back</a></div>
                    </div>
                    <hr />
                    <form id="form" name="form" method="post" enctype="multipart/form-data">
                        <div class="buttons">
                            <div class="upload-button">
                                <input id="files" name="files" type="file" size="50" onchange="uploadFiles('files');" class="form-control" />
                            </div>
                        </div>
                    </form>
                    <div class="row g-3 my-4 d-none">
                        <div class="col-6 text-end d-none">
                            <button class="btn btn-primary w-50">Upload Resume</button>
                        </div>
                        <div class="col-6">
                            <button class="btn btn-primary w-50" onclick="location.href='@Url.Action("CheckCompatibiltyAll", "CheckCompatibiltyAll")';return false;">Check Compatibility</button>
                        </div>
                    </div>
                    <div class="tab-class text-center wow fadeInUp" data-wow-delay="0.3s">

                        <div class="tab-content">
                            <div id="tab-1" class="tab-pane fade show p-0 active">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- View All Resume - Vacancy Wise -->
<script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
<script src="~/js/ViewAllResumeImplemetation.js"></script>


